{
  "name": "fa-checkbox",
  "author": "Cypriot Free Software Foundation",
  "license": "MIT",
  "version": "0.0.0-semantic-release",
  "description": "CSS and SALL lib that shows a shows beautifull checkboxed using fontawesome",
  "main": "index.html",
  "keywords": [
    "css",
    "checkbox",
    "fontawesome"
  ],
  "scripts": {
    "start": "npm-run-all --parallel watch-css start-server",
    "watch-css": "sass --watch scss:dev",
    "start-server": "live-server",
    "sass": "sass scss:dist",
    "prepare": "husky install",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run commitmsg"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.7",
    "semantic-release": "^23.0.0"
  },
  "dependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6"
  }
}
